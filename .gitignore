.env

# Ignore contents of workspace directory but keep the directory
workspace/*
!workspace/.gitkeep

# Ignore contents of data directory but keep the directory
data/*
!data/.gitkeep

# Ignore contents of out directory but keep the directory
out/*
!out/.gitkeep

# Ignore contents of out directory but keep the directory
models/*
